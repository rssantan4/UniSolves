#include <stdio.h>

int main() {

int num1, num2;

printf("Digite seu primeiro valor: ");
scanf("%d", &num1);

printf("Digite seu segundo valor: ");
scanf("%d", &num2);

if (num1 > num2) {
    printf("Seu primeiro valor digitado é maior: %d", num1);

} else {
    printf("Seu segundo valor digitado é maior: %d", num2);
}

return 0;

}