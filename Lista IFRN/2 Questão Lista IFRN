#include <stdio.h>

int main(){
    int v[15], i, soma;
    for (i = 0;i < 15; i++) {
        printf("Insira o %dº valor do vetor: ", i+1);
        scanf("%d", &v[i]);
        fflush(stdin);
    }
    for (i = 0; i < 15; i++) {
      soma += v[i];
    }
    printf("Essa é a soma do vetor de 15 posições: %d", soma);
    return 0;
}