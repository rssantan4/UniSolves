#include <stdio.h>

int main(){

int v[20], i, cont;

for (i = 0; i < 20; i++) {
    printf("Insira o %dº valor: ", i+1);
    scanf("%d", &v[i]);
    fflush(stdin);
}
for (i = 0; i < 20; i++) {
    if (v[i] > 8) {
        cont++;
    }
}
printf("%d numero(s) no vetor são maiores que 8", cont);
}